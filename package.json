{
  "name": "frontend-interview",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "concurrently \"npm:server\" \"npm:client\"",
    "client": "parcel src/index.html",
    "server": "json-server --watch db.json",
    "build": "parcel build src/index.html"
  },
  "devDependencies": {
    "concurrently": "^7.1.0",
    "json-server": "^0.17.1",
    "parcel": "^2.5.0"
  }
}
